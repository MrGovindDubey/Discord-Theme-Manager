{
  "manifest_version": 3,
  "name": "Discord Theme Manager",
  "version": "1.0",
  "description": "Adds a menu to switch between Discord themes",
  "author": "Mr. Govind Dubey",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
